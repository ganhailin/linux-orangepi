&pcie2x1l2 {
	reset-gpios = <&gpio3 RK_PD1 GPIO_ACTIVE_HIGH>;
	vpcie3v3-supply = <&vcc3v3_pcie2x1l2>;
	rockchip,skip-scan-in-resume;
	status = "okay";
};

&pinctrl
{
	gpio-func {
		leds_gpio: leds-gpio {
			rockchip,pins =
				<1 RK_PA2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
};
